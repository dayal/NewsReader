%div.container
  %div.row
    %div.span9
      - if !@feeds_list.feeds.empty?
        %div.offset1.span5.well
          %h5 Current feeds in your FeedsList
          - @feeds_list.feeds.each do |feed|
            %p
              = feed.name
              = link_to "Delete Feed", remove_feed_user_feeds_list_path(@user, @feeds_list, feed_id: feed.id)
      = form_for(@feeds_list, :url => user_feeds_list_path(@user), :html => { :class => "offset1 span6" }) do |f|
        = render :partial => "feedslist_errors"
        = render :partial => "feedslist_form", :locals => { :f => f }
        = link_to "Back to Profile Page", user_path(@user), :class => "pull-left btn btn-inverse"
        %p= f.submit "Save Changes", :class => "pull-right btn btn-inverse"
    %div.span3
      = render :partial => "shared/right_column"
