%div.container
  %div.row
    %div.span12
      = form_for(:session, :url => sessions_path, :html => {:class => "offset3 span5 offset4"}) do |f|
        = render :partial => "shared/login_errors"
        = f.label :email
        = f.text_field :email, :class => "input-block-level"

        = f.label :password
        = f.password_field :password, :class => "input-block-level"

        %p= f.submit "Log In", :class => "pull-right btn btn-inverse"
