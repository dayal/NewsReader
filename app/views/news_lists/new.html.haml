%div.container
  %div.row
    %div.span9
      = form_for(@news_list, :url => user_news_lists_path(@user), :html => { :class => "offset1 span6" }) do |f|
        = render :partial => "newslist_errors"
        = f.label :name
        = f.text_field :name, :class => "input-block-level"
        = link_to "Back to Profile Page", user_path(@user), :class => "pull-left btn btn-inverse"
        %p= f.submit "Create NewsList", :class => "pull-right btn btn-inverse"
    %div.span3
      = render :partial => "shared/right_column"
